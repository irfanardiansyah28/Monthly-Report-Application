            Dim ekioskcount As Long
            Dim loskprcount As Long
            Dim lossmecount As Long
            
                Set uniquecaseid = CreateObject("Scripting.Dictionary")
            
        For i = 2 To lastrow
        
            caseid = sourcesheet.Cells(i, 11).Value
            product = sourcesheet.Cells(i, 10).Value
            status = sourcesheet.Cells(i, 19).Value
            react = sourcesheet.Cells(i, 14).Value
            cmcolumn = sourcesheet.Cells(i, "CM").Value
            verihubs = sourcesheet.Cells(i, "O").Value
            cpcolumn = sourcesheet.Cells(i, "CP").Value

            If status = "NOT_FRAUD" And react <> "Reactivated" And IsError(cpcolumn) And IsError(cmcolumn) And verihubs = "N" Then
            
                If Not uniquecaseid.exists(caseid) Then
                    uniquecaseid.Add caseid, 1
                    
                    Select Case product
                        Case "CBM"
                            cbmcount = cbmcount + 1
                                                    
                        Case "EKI"
                            ekioskcount = ekioskcount + 1
                            
                        Case "LOSKPR"
                            loskprcount = loskprcount + 1
                            
                        Case "LOSSME"
                            lossmecount = lossmecount + 1
                    End Select
                End If
            End If
        Next i
        
        destsheet.Range("G52").Value = cbmcount
        destsheet.Range("G53").Value = ekioskcount
        destsheet.Range("G55").Value = loskprcount
        destsheet.Range("G54").Value = lossmecount
        
End Sub

