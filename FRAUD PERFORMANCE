Sub SLAAllChannel()
    Dim sourcesheet As Worksheet
    Dim destsheet As Worksheet
    Dim lastrow As Long
    Dim cmcolumn As Variant
    Dim product As Variant
    
        Set sourcesheet = ThisWorkbook.Sheets("APPLICATION")
        Set destsheet = ThisWorkbook.Sheets("FDU - App")
        
        lastrow = sourcesheet.Cells(sourcesheet.Rows.Count, "CK").End(xlUp).Row
        
            Dim cbmcount As Long
            Dim ekioskcount As Long
            Dim loskprcount As Long
            Dim lossmecount As Long
            
        For i = 2 To lastrow
        
            product = sourcesheet.Cells(i, 14).Value
            react = sourcesheet.Cells(i, "CK").Value
            reactdatetime = sourcesheet.Cells(i, "H").Value
            cmcolumn = sourcesheet.Cells(i, "CM").Value
            cpcolumn = sourcesheet.Cells(i, "CP").Value

            If product <> "Reactivated" And IsError(cpcolumn) And IsError(cmcolumn) Then
                    
                    Select Case react
                        Case "a. 1 day"
                            cbmcount = cbmcount + 1
                                                    
                        Case "b. 1-2 days"
                            ekioskcount = ekioskcount + 1
                            
                        Case "c. 2-3 days"
                            loskprcount = loskprcount + 1
                            
                        Case "d. >3 days"
                            lossmecount = lossmecount + 1
                    End Select
            End If
        Next i
        
        destsheet.Range("C82").Value = cbmcount
        destsheet.Range("C83").Value = ekioskcount
        destsheet.Range("C84").Value = loskprcount
        destsheet.Range("C85").Value = lossmecount
        
End Sub


Sub SLAAllChannelExcludeVerihubs()
    Dim sourcesheet As Worksheet
    Dim destsheet As Worksheet
    Dim lastrow As Long
    Dim cmcolumn As Variant
    Dim product As Variant
    
        Set sourcesheet = ThisWorkbook.Sheets("APPLICATION")
        Set destsheet = ThisWorkbook.Sheets("FDU - App")
        
        lastrow = sourcesheet.Cells(sourcesheet.Rows.Count, "CK").End(xlUp).Row
        
            Dim cbmcount As Long
            Dim ekioskcount As Long
            Dim loskprcount As Long
            Dim lossmecount As Long
            
