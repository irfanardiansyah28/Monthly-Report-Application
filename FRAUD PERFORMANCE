Sub AutoDecline()
    Dim sourcesheet As Worksheet
    Dim destsheet As Worksheet
    Dim lastrow As Long
    Dim caseid As Variant
    Dim product As Variant
    Dim status As Variant
    Dim cmcolumn As Variant
    Dim uniquecaseid As Object
    
        Set sourcesheet = ThisWorkbook.Sheets("RULE NO CASE")
        Set destsheet = ThisWorkbook.Sheets("FDU - App")
        
        lastrow = sourcesheet.Cells(sourcesheet.Rows.Count, "D").End(xlUp).Row
        
            Dim cbmcount As Long
            Dim ekioskcount As Long
            Dim loskprcount As Long
            Dim lossmecount As Long
            
                Set uniquecaseid = CreateObject("Scripting.Dictionary")
            
        For i = 2 To lastrow
        
            caseid = sourcesheet.Cells(i, 4).Value
            product = sourcesheet.Cells(i, 5).Value
            status = sourcesheet.Cells(i, 6).Value
            
            If status = "Decline" Then
            
                If Not uniquecaseid.exists(caseid) Then
                    uniquecaseid.Add caseid, 1
                    
                    Select Case product
                        Case "CBM"
                            cbmcount = cbmcount + 1
                                                    
                        Case "EKI"
                            ekioskcount = ekioskcount + 1
                            
                        Case "LOSKPR"
                            loskprcount = loskprcount + 1
                            
                        Case "LOSSME"
                            lossmecount = lossmecount + 1
                    End Select
                End If
            End If
        Next i
        
        destsheet.Range("D7").Value = cbmcount
        destsheet.Range("D10").Value = ekioskcount
        destsheet.Range("D9").Value = loskprcount
        destsheet.Range("D8").Value = lossmecount
End Sub

Sub AFMConfirmFraud()
    Dim sourcesheet As Worksheet
    Dim destsheet As Worksheet
    Dim lastrow As Long
    Dim caseid As Variant
    Dim product As Variant
    Dim status As Variant
    Dim cmcolumn As Variant
    Dim uniquecaseid As Object
    
        Set sourcesheet = ThisWorkbook.Sheets("APPLICATION")
        Set destsheet = ThisWorkbook.Sheets("FDU - App")
        
        lastrow = sourcesheet.Cells(sourcesheet.Rows.Count, "K").End(xlUp).Row
        
            Dim cbmcount As Long
            Dim ekioskcount As Long
            Dim loskprcount As Long
            Dim lossmecount As Long
            
                Set uniquecaseid = CreateObject("Scripting.Dictionary")
            
        For i = 2 To lastrow
        
            caseid = sourcesheet.Cells(i, 11).Value
            product = sourcesheet.Cells(i, 10).Value
            status = sourcesheet.Cells(i, 19).Value
            react = sourcesheet.Cells(i, 14).Value
            cmcolumn = sourcesheet.Cells(i, "CM").Value
            cpcolumn = sourcesheet.Cells(i, "CP").Value

            If status = "CONFIRMED_FRAUD_PERPETRATOR" And react <> "Reactivated" And IsError(cmcolumn) And IsError(cpcolumn) Then
            
                If Not uniquecaseid.exists(caseid) Then
                    uniquecaseid.Add caseid, 1
                    
                    Select Case product
                        Case "CBM"
                            cbmcount = cbmcount + 1
                                                    
                        Case "EKI"
                            ekioskcount = ekioskcount + 1
                            
                        Case "LOSKPR"
                            loskprcount = loskprcount + 1
                            
                        Case "LOSSME"
                            lossmecount = lossmecount + 1
                    End Select
                End If
            End If
        Next i
        
        destsheet.Range("C41").Value = cbmcount
        destsheet.Range("C42").Value = ekioskcount
        destsheet.Range("C44").Value = loskprcount
        destsheet.Range("C43").Value = lossmecount
        
End Sub


