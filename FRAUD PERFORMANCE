            product = sourcesheet.Cells(i, 14).Value
            react = sourcesheet.Cells(i, 89).Value
            reactdatetime = sourcesheet.Cells(i, "H").Value
            cmcolumn = sourcesheet.Cells(i, "CM").Value
            verihubs = sourcesheet.Cells(i, "O").Value
            cpcolumn = sourcesheet.Cells(i, "CP").Value
            

            If product <> "Reactivated" And IsError(cmcolumn) And IsError(cpcolumn) And verihubs = "N" And reactdatetime = "NULL" Then
                    
                    Select Case react
                        Case "a. 1 day"
                            cbmcount = cbmcount + 1
                                                    
                        Case "b. 1-2 days"
                            ekioskcount = ekioskcount + 1
                            
                        Case "c. 2-3 days"
                            loskprcount = loskprcount + 1
                            
                        Case "d. >3 days"
                            lossmecount = lossmecount + 1
