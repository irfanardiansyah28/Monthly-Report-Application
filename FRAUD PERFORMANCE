Sub RUNFRAUDPERFORM()
    Call ApplicationIn
    Call ApplicationInExcludeVerihubs
    Call TotalAlertAFM
    Call TotalAlertExcludeVerihubs
    Call TotalAlertAFMExcludeVerihubs
    Call AutoDecline
    Call AFMConfirmFraud
    Call AFMConfirmFraudVictim
    Call AFMConfirmCuspiciousDecline
    Call AFMNotFraud
    Call AFMConfirmFraudExcludeVerihubs
    Call AFMConfirmFraudVictimExcludeVerihubs
    Call AFMConfirmCuspiciousDeclineExcludeVerihubs
    Call AFMNotFraudExcludeVerihubs
    Call FraudRateTotalFraud
    Call FraudRateTotalFraudExcludeVerihubs
    Call SLAAllChannel
    Call SLAAllChannelExcludeVerihubs
    Call SLACBMChannel
    Call SLACBMChannelExcVerihubs
    Call SLAEkioskhannel
    Call SLALOSMortgage
    Call SLALOSSME
    Call AFMManualFraud
    Call AFMManualSuspicious
    Call AFMManualNotFraud
    MsgBox "Done Ya, Thank you!!!"
End Sub
Sub ApplicationIn()
    Dim wssource As Worksheet
    Dim wsresult As Worksheet
    Dim caseid As Variant
    Dim product As Variant
    Dim lastrow As Long
    Dim i As Long
    Dim uniquecaseid As Object
    
        Set wssource = ThisWorkbook.Sheets("APPLICATION")
        Set wsresult = ThisWorkbook.Sheets("FDU - App")
        
        lastrow = wssource.Cells(wssource.Rows.Count, "I").End(xlUp).Row
        
            Dim cbmcount As Long
            Dim ekioskcount As Long
            Dim loskprcount As Long
            Dim lossmecount As Long
            
                Set uniquecaseid = CreateObject("Scripting.Dictionary")
        
        For i = 2 To lastrow
            
            caseid = wssource.Cells(i, 9).Value
            product = wssource.Cells(i, 10).Value
            
                If Not uniquecaseid.exists(caseid) Then
                    uniquecaseid.Add caseid, 1
                    
                    Select Case product
                        Case "CBM"
                            cbmcount = cbmcount + 1
                                                    
                        Case "EKI"
                            ekioskcount = ekioskcount + 1
                            
                        Case "LOSKPR"
                            loskprcount = loskprcount + 1
                            
                        Case "LOSSME"
                            lossmecount = lossmecount + 1
                    End Select
                End If
        Next i
        
        wsresult.Cells(7, 3).Value = cbmcount
        wsresult.Cells(10, 3).Value = ekioskcount
        wsresult.Cells(9, 3).Value = loskprcount
        wsresult.Cells(8, 3).Value = lossmecount
End Sub

Sub ApplicationInExcludeVerihubs()
    Dim sourcesheet As Worksheet
    Dim destsheet As Worksheet
    Dim lastrow As Long
    Dim caseid As Variant
    Dim product As Variant
    Dim status As Variant
    Dim cmcolumn As Variant
    Dim uniquecaseid As Object
    
        Set sourcesheet = ThisWorkbook.Sheets("APPLICATION")
        Set destsheet = ThisWorkbook.Sheets("FDU - App")
        
        lastrow = sourcesheet.Cells(sourcesheet.Rows.Count, "D").End(xlUp).Row
        
            Dim cbmcount As Long
            Dim ekioskcount As Long
            Dim loskprcount As Long
            Dim lossmecount As Long
            
                Set uniquecaseid = CreateObject("Scripting.Dictionary")
            
        For i = 2 To lastrow
        
            caseid = sourcesheet.Cells(i, 9).Value
            product = sourcesheet.Cells(i, 10).Value
            status = sourcesheet.Cells(i, 15).Value
            
            If status = "N" Then
            
                If Not uniquecaseid.exists(caseid) Then
                    uniquecaseid.Add caseid, 1
                    
                    Select Case product
                        Case "CBM"
                            cbmcount = cbmcount + 1
                                                    
                        Case "EKI"
                            ekioskcount = ekioskcount + 1
                            
                        Case "LOSKPR"
                            loskprcount = loskprcount + 1
                            
                        Case "LOSSME"
                            lossmecount = lossmecount + 1
                    End Select
                End If
            End If
        Next i
        
        destsheet.Range("C14").Value = cbmcount
        destsheet.Range("C17").Value = ekioskcount
        destsheet.Range("C16").Value = loskprcount
        destsheet.Range("C15").Value = lossmecount
End Sub

Sub TotalAlertAFM()
    Dim sourcesheet As Worksheet
    Dim destsheet As Worksheet
    Dim lastrow As Long
    Dim caseid As Variant
    Dim product As Variant
    Dim status As Variant
    Dim cmcolumn As Variant
    Dim uniquecaseid As Object
    
        Set sourcesheet = ThisWorkbook.Sheets("APPLICATION")
        Set destsheet = ThisWorkbook.Sheets("FDU - App")
        
        lastrow = sourcesheet.Cells(sourcesheet.Rows.Count, "K").End(xlUp).Row
        
            Dim cbmcount As Long
            Dim ekioskcount As Long
            Dim loskprcount As Long
            Dim lossmecount As Long
            
                Set uniquecaseid = CreateObject("Scripting.Dictionary")
            
        For i = 2 To lastrow
        
            caseid = sourcesheet.Cells(i, 11).Value
            product = sourcesheet.Cells(i, 10).Value
            status = sourcesheet.Cells(i, 14).Value
            cmcolumn = sourcesheet.Cells(i, "CM").Value
            cpcolumn = sourcesheet.Cells(i, "CP").Value
            
            If status <> "Reactivated" And IsError(cmcolumn) And IsError(cpcolumn) Then
            
                If Not uniquecaseid.exists(caseid) Then
                    uniquecaseid.Add caseid, 1
                    
                    Select Case product
                        Case "CBM"
                            cbmcount = cbmcount + 1
                                                    
                        Case "EKI"
                            ekioskcount = ekioskcount + 1
                            
                        Case "LOSKPR"
                            loskprcount = loskprcount + 1
                            
                        Case "LOSSME"
                            lossmecount = lossmecount + 1
                    End Select
                End If
            End If
        Next i
        
        destsheet.Range("F7").Value = cbmcount
        destsheet.Range("F10").Value = ekioskcount
        destsheet.Range("F9").Value = loskprcount
        destsheet.Range("F8").Value = lossmecount
        
        destsheet.Range("C64").Value = cbmcount
        destsheet.Range("C65").Value = ekioskcount
        destsheet.Range("C67").Value = loskprcount
        destsheet.Range("C66").Value = lossmecount
        
End Sub

